<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
<ignoreVersions>
    <ignoreVersion type="regex">.*-beta.</ignoreVersion>
    <ignoreVersion type="regex">.*-ALPHA.*</ignoreVersion>
    <ignoreVersion type="regex">.*-Alpha.*</ignoreVersion>
    <ignoreVersion type="regex">.*\\.Alpha.*</ignoreVersion>
    <ignoreVersion type="regex">.*-alpha.*</ignoreVersion>         
    <ignoreVersion type="regex">.*-rc.*</ignoreVersion>
    <ignoreVersion type="regex">.*-RC.*</ignoreVersion>
    <ignoreVersion type="regex">.*-preview.*</ignoreVersion>
    <ignoreVersion type="regex">.*-b.*</ignoreVersion>
    <ignoreVersion type="regex">.*-M.*</ignoreVersion>
    <ignoreVersion type="regex">.*\\.M\\d+</ignoreVersion>
         <ignoreVersion type="regex">.*\\.jre11-.*</ignoreVersion>
         <ignoreVersion type="regex">.*\\.jre\\d+</ignoreVersion>
         <ignoreVersion type="regex">.*\\.jbossorg-\\d+</ignoreVersion>
         <ignoreVersion type="regex">.*[-_\.](alpha|Alpha|ALPHA|b|beta|Beta|BETA|rc|RC|M|EA)[-_\.]?[0-9]*</ignoreVersion>
         </ignoreVersions>
</ruleset>
